<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fir_filing_system">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="Admin.AdminDigitalSignature"></activity>
        <activity android:name="Initial.WelcomeActivity" />
        <activity android:name="MenuActivities.KnowYourPoliceActivity" />
        <activity android:name="MenuActivities.FAQActivity" />
        <activity android:name="MenuActivities.CheckStatusActivity" />
        <activity android:name="MenuActivities.ReportCrimeActivity" />
        <activity android:name="MenuActivities.Fill_FIR_Activity" />
        <activity android:name="Initial.ForgotPasswordActivity" />
        <activity android:name="Initial.LoginActivity" />
        <activity android:name="Initial.RegisterActivity" />
        <activity android:name="Initial.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>